<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TaxAmountOK__c</fullName>
    <description>세액단가 * 확정수량</description>
    <formula>IF(OR(ISBLANK(TaxPrice__c) , ISBLANK(QuantityConfirm__c)),
    TaxAmount__c,
    (TaxPrice__c * QuantityConfirm__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Tax Amount OK</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
